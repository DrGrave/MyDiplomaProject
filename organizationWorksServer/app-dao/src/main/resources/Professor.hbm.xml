<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sethomecompany.domain">

    <class name="Professor" table="professor">
        <id name="idProfessor" column="idprofessor">
            <generator class="native"/>
        </id>
        <property name="professorName" column="ProfessorName"/>
        <property name="professorPatronum" column="ProfessorPatronum"/>
        <property name="professorSurname" column="ProfessorSurname"/>
    </class>


</hibernate-mapping>