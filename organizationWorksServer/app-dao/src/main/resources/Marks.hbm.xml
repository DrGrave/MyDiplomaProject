<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sethomecompany.domain">

    <class name="Marks" table="marks">
        <id name="idMark" column="idmark">
            <generator class="native"/>
        </id>
        <property name="mark" column="Mark"/>
        <many-to-one name="work" column="idofwork" class="Work" not-null="true"/>
        <many-to-one name="userOfMark" column="idStudent" class="User" not-null="true"/>
        <many-to-one name="timeteable" column="IdTimeteable" class="Timeteable" not-null="true"/>
    </class>


</hibernate-mapping>