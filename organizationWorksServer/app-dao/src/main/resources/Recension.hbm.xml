<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sethomecompany.domain">

    <class name="Recension" table="recension">
        <id name="idRecension" column="idrecension">
            <generator class="native"/>
        </id>
        <property name="textRecension" column="texrecension"/>
        <property name="accapted" column="acapted"/>
        <many-to-one name="queue" column="idqueue" class="Queue" not-null="true"/>
        <many-to-one name="userRecension" column="idStudent" class="User" not-null="true"/>
    </class>


</hibernate-mapping>