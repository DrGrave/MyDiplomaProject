<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sethomecompany.domain">

    <class name="Timeteable" table="timeteable">
        <id name="idTimeteable" column="idtimeteable">
            <generator class="native"/>
        </id>
        <property name="dateOfWork" column="dateofwork"/>
        <property name="timeOfEndWork" column="timeofendwork"/>
        <property name="timeOfWork" column="timeofstartwork"/>
        <many-to-one name="professor" column="idprofessor" class="Professor" not-null="true"/>
        <property name="auditory" column="auditory"/>
        <many-to-one name="subject" column="IdSubject" class="Subject" not-null="true"/>
    </class>


</hibernate-mapping>