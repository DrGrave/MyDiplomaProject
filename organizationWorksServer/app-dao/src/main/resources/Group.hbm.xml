<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sethomecompany.domain">

    <class name="Group" table="studentgroup">
        <id name="idGroup" column="IdGroup">
            <generator class="native"/>
        </id>
        <property name="studentStarosta" column="idStudentStarosta"/>
        <property name="numberOfGroup" column="NumberOfGroup"/>
    </class>


</hibernate-mapping>