<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sethomecompany.domain">

    <class name="Attorney" table="attorney">
        <id name="idAttorney" column="idAttornery">
            <generator class="native"/>
        </id>
        <many-to-one name="professor" column="idprofessor" class="Professor" not-null="true"/>
        <many-to-one name="userAttorney" column="idStudent" class="User" not-null="true"/>
        <many-to-one name="subject" column="IdSubject" class="Subject" not-null="true"/>
        <many-to-one name="typeOfAttorney" column="IdTypeOfAttorney" class="TypeOfAttorney" not-null="true"/>
    </class>


</hibernate-mapping>